{
  "tasks": {
    "bundle": "npm run build",
    "serve": "deno run --allow-net --allow-read=dist jsr:@std/http/file-server dist",
    "server": "deno run --allow-net --allow-read --allow-env server/main.ts",
    "dev": "npm run dev",
    "build": "npm run build",
    "preview": "npm run preview"
  },
  "imports": {
    "@hono/hono": "jsr:@hono/hono@^4.6.15",
    "solid-js": "npm:solid-js@^1.3.10",
    "solid-app-router": "npm:solid-app-router@^0.3.1",
    "lodash": "npm:lodash@^4.17.21",
    "tabbable": "npm:tabbable@^5.3.1"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js"
  },
  "permissions": {
    "default": {
      "net": true,
      "read": ["dist"]
    }
  }
}
